<div class="col-md-12">
  <div class="booking">
    <%= link_to("Book New Seat", '#', class: "btn btn-primary pull-left") %>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>Name</td>
        <td>Desk</td>
        <td>DateFrom</td>
        <td>DateTo</td>
      </tr>
    </thead>
    <tbody>
      <% @bookings.each do |booking| %>
        <tr>
          <td><%= booking.name %></td>
          <td><%= booking.desk %></td>
          <td><%= booking.date_from %></td>
          <td><%= booking.date_to %></td>
          <td>
              <%= link_to("View", '#', class: "btn btn-sm btn-default")%>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
